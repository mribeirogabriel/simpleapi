{
  "name": "simpleapi",
  "version": "1.0.0",
  "description": "Simple API using NodeJS + TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "build": "tsc --project ./"
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "author": "https://github.com/mribeirogabriel/",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.17.2"
  }
}
